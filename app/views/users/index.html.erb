<%= render 'books/list', book: @book, user: @user %>

<h1>Users</h1>
<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Name</th>
    </tr>
  </thead>

  <tboudy>
    <tr>
      <% @users.each do |user| %>
      <td><%= attachment_image_tag user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg"%></td>

      <td><%= user.name %></td>
      <td><%= link_to 'Show', user_path(user) %></td>
    </tr>
    <% end %>
  </tboudy>
</table>